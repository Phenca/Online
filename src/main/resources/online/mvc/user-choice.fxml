<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="online.mvc.Controller">
    <children>
        <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="30.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Label text="Bienvenue ! Que souhaitez-vous faire ?">
                    <font>
                        <Font name="Impact" size="25.0" />
                    </font>
                </Label>
                <Button mnemonicParsing="false" onAction="#display_main" prefHeight="50.0" prefWidth="250.0" text="Faire une nouvelle commande" />
                <Button mnemonicParsing="false" onAction="#display_orders" prefHeight="50.0" prefWidth="250.0" text="Suivre mes commandes" />
            </children>
        </VBox>
    </children>
</AnchorPane>
